		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",

			"client": {
				"$path": "client"
			},

			"shared": {
				"$path": "shared"
			},

			"packages": {
				"$path": "packages"
			}
		},

		"ServerStorage": {
			"$className": "ServerStorage",

			"server": {
